{
  "extends": "solium:all",
  "plugins": [
    "security"
  ],
  "rules": {
    "quotes": [
      0
    ],
    "mixedcase": [
      0
    ],
    "indentation": [
      "error",
      4
    ],
    "arg-overflow": [
      0
    ],
    "lbrace": [
      0
    ],
    "security/no-block-members": [
      0
    ]
  }
}
